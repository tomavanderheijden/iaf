<ibis>
	<adapter name="Log" description="Gets users" >

		<receiver
			className="nl.nn.adapterframework.receivers.GenericReceiver"
			name="Log"
		>
			<listener 
				className="nl.nn.adapterframework.http.rest.ApiListener"
				uriPattern="mgt/log/{iets}"
				method="GET"
				produces="JSON"
			/>
		</receiver>
		
		<pipeline firstPipe="result">
		
			<exits>
				<exit path="OK" state="success" code="200"/>
				<exit path="NotModified" state="success" code="304" empty="true" />
				<exit path="BadRequest" state="error" code="400" empty="true" />
				<exit path="NotAuthorized" state="error" code="401" empty="true" />
				<exit path="NotAllowed" state="error" code="403" empty="true" />
				<exit path="ServerError" state="error" code="500" empty="true" />
			</exits>
			
			<pipe
				name="result"
				className="nl.nn.adapterframework.pipes.FixedResult"
				returnString='[{"key":"servername","values":[null,"DDCCS","DDCIM","nl001lapvm1018","nl001lapvm1021","nl001lapvm105_851","nl001lapvm106_851","nl001lapvm106_851/ddcis_cluster","nl001lapvm107_852","nl001lapvm107_853","nl001lapvm108_853","nl020lapvm1222"]},{"key":"personeelsnummer","values":[null,"d000001","d000003","d000004","d000005","d000006","d000007","d000008","d000010","d000013","d000015","d000018","d000501","d000502","d000503","d000504","d000505","d000508","d000509","d000510","d000615","d220472","d306127","d361313"]},{"key":"mgtLogEntryType","values":["REST_CALL_ERROR","FRONT_END_LOG","DDCCS_CONVERSION_FAILURE","DDCCS_CONVERSION_ERROR","DDCIS_MDB_REPLY_FAILURE","DDCIS_CLEANUP_INFO","DDCIM_LOG","DDCIM_MDB_REPLY_FAILURE","DDCIS_ERROR","PUB_NOT_MDB_REQ_FAILURE","DDCIS_AUTO_MBX_INFO"]},{"key":"severity","values":["UNKNOWN","DEBUG","INFO","WARN","ERROR","FATAL"]}]'
			>
				<forward name="success" path="OK" />
			</pipe>

		</pipeline>
	</adapter>
</ibis>
